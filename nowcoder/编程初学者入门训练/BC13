public class Bc13 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int[] arr = {73, 32, 99, 97, 110, 32, 100, 111, 32, 105, 116 , 33};
		
		for(int i=0;i<arr.length;i++) {
			System.out.print((char)arr[i]);
		}

	}

}
